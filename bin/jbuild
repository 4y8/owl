(jbuild_version 1)

(executable (
  (name       zoo)
  (public_name owl)
  (libraries  (
    compiler-libs.toplevel
    owl
    owl_zoo
  ))
  (link_flags (-linkall))
  (modes      (byte))
))

(install (
  (section bin)
  (files (
    (owl_upload_gist.sh as owl_upload_gist.sh)
    (owl_download_gist.sh as owl_download_gist.sh)
    (owl_list_gist.sh as owl_list_gist.sh)
  ))
))
